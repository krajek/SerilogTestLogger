<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Artur Krajewski</Authors>
    <Company />
    <Product />
    <Description>In-memory Serilog logger intended for automatic tests</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/krajek/SerilogTestLogger</PackageProjectUrl>
    <RepositoryUrl>https://github.com/krajek/SerilogTestLogger</RepositoryUrl>
    <PackageTags>serilog, logging, tests</PackageTags>
    <Version>1.3.0</Version>
    <AssemblyName>Serilog.TestLogger</AssemblyName>
    <RootNamespace>Serilog.TestLogger</RootNamespace>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Serilog" Version="2.5.0" />
  </ItemGroup>

</Project>
